   "source": [
    "# Solution\n",
    "A = (2, 4)  # Vector\n",
    "b = 3       # Scalar \n",
    "\n",
    "def dot(A, b):\n",
    "    return tuple(x * b for x in A)\n",
    "\n",
    "dot(A,b)"
