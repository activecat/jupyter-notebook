    "# To sum up the even numbers from 2 to 100 inclusive. \n",
    "from functools import reduce\n",
    "\n",
    "numbers = [x for x in range(1, 101) if x % 2 == 0]\n",
    "print(\"numbers =\", numbers, '\\n')\n",
    "\n",
    "answer = reduce(lambda x,y: x+y, numbers)\n",
    "print(\"answer =\", answer)"
